#!groovy

properties([disableConcurrentBuilds()])

pipeline {
    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
   stages {
        stage("First step") {
            steps {
                sh 'ssh root@192.168.1.37 \'cd /root/test1 \''
                sh 'ls -al'
                
            }
        }
        stage("Second step") {
            steps {
                echo 'finish'
            }
        }
    }
}
